<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id = "mathcheatobjectvalue" class = "com.tns.springdiforobj.MathCheat" />
	
	<bean id ="s1" class ="com.tns.springdiforobj.Student">  
	<property name ="cheat" ref="mathcheatobjectvalue" />
	</bean>
	
	<bean id ="s2" class ="com.tns.springdiforobj.AnotherStudent">  
	<property name ="cheat" ref="mathcheatobjectvalue" />
	</bean>

</beans>